</xs:choice>
</xs:sequence>
</xs:choice>
</xs:sequence>
</xs:complexType>
</xs:element>
<xs:element name="performance">
<xs:complexType>
<xs:sequence>
<xs:element ref="cruiseSpeed"/>
<xs:element ref="maxSpeed"/>
<xs:element ref="range"/>
<xs:choice>
<xs:element ref="fuelFlow"/>
<xs:element ref="energyFlow"/>
</xs:choice>
<xs:element ref="dragCoefficient"/>
